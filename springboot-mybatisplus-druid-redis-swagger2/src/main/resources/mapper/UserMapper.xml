<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootmybatisplus.dao.UserMapper">

  <!-- 通用查询映射结果 -->
  <resultMap id="BaseResultMap" type="com.example.springbootmybatisplus.entity.User">
    <result column="username" property="username" />
    <result column="password" property="password" />
  </resultMap>

  <select id="getUser" resultType="com.example.springbootmybatisplus.entity.User">
  select * from user
  </select>
</mapper>
